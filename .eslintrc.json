{
  "extends": ["react-app","plugin:jsx-a11y/recommended","prettier/react","plugin:import/errors","plugin:import/warnings"],
  "plugins": ["jsx-a11y","prettier","import","immutable"],
  "rules": {
    "prettier/prettier": "error",
    "import/no-unresolved": [2, {"commonjs": true, "amd": true}],
     "import/named": 2,
     "import/default": 2,
     "import/export": 2,
     "import/no-extraneous-dependencies":["error", {"devDependencies": false, "optionalDependencies": false, "peerDependencies": false}],
     "immutable/no-let": 2
  }
}