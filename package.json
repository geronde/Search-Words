{
  "name": "reactboilerplate",
  "version": "1.5.3",
  "description": "reactboilerplate for development and production",
  "scripts": {
    "webpack": "webpack",
    "clean": "rimraf ./build/*",
    "dev": "set NODE_ENV='development' & webpack-dev-server --colors",
    "prod:server": "node production.server.js",
    "prod:client": "set NODE_ENV='production' & webpack --config webpack.production.config.js --progress --colors ",
    "build": "npm run prod:client & npm run prod:server",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "L",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.7",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "foreman": "^2.0.0",
    "image-webpack-loader": "^3.4.1",
    "leven": "^2.1.0",
    "nodemon": "^1.17.1",
    "postcss-loader": "^2.0.7",
    "prettier": "^1.11.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "svg-url-loader": "^2.3.1",
    "url-loader": "^0.5.9",
    "word-list": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "connect-history-api-fallback": "^1.4.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "morgan": "^1.9.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "express": "^4.16.2"
  }
}
